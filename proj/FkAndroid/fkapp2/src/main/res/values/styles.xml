<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="fluentuiProgressPrimaryColor">@color/colorAccent</item>
        <item name="fluentuiProgressBackgroundColor">@color/colorPrimaryDark</item>
        <item name="fluentuiContextualCommandBarBackgroundColorSelected">@color/white</item>
        <item name="fluentuiContextualCommandBarIconTintSelected">@color/colorAccent</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">true</item>
    </style>

    <style name="AppTheme" parent="AppBaseTheme" />

</resources>
